# Declare parameters wE and wF

Param wE = 17;
Param wF = 7; 

# Name your source file name

Name dagOpTest; 

# Declare operators you will use as well as operator parameters

Operator Normalizer name = myNorm wX = $wE wR = $wF maxShift = 17;

# Declare I/O Signals

Input A:Float($wE,$wF);
Input B:Float($wE,$wF);
Output S:Float($wE,$wF);
Output Z:Float($wE,$wF);

# Assemble the operators in the order of your preference

Z = myNorm(A,B);
S = myNorm(A,B);

